<div class="container">
	<h4 class = "center-align">All Charts (Played: <%= @user.userstats.count %>/<%= Difficulty.count %>)</h4>
	<div class="row">
		<div class="col s12 l8 offset-l2">
			<ul class="collapsible popout" data-collapsible="accordion">
				<% for x in 0..3 do %>
					<li>
					<div id = <%= "titles_#{x}" %> class="collapsible-header" onclick="userstat_update()"><i id = "temp" class="material-icons">place</i><%= @titles[x] %></div>
					<div class="collapsible-body">
						<div class="row">
							<div class="col s12 l6">
								<img id = <%= "failed" %> src= <%= asset_url "clears/failed.png" %> />: <%= @failed[x].count %></br>
								<img id = <%= "cleared" %> src= <%= asset_url "clears/cleared.png" %> />: <%= @cleared[x].count %></br>
								<img id = <%= "excessive" %> src= <%= asset_url "clears/excessive.png" %> />: <%= @excessive[x].count %></br>
								<img id = <%= "UC" %> src= <%= asset_url "clears/UC.png" %> />: <%= @UC[x].count %></br>
								<img id = <%= "PUC" %> src= <%= asset_url "clears/PUC.png" %> />: <%= @PUC[x].count %></br>
							</div>
							<div class="col s12 l6">
								<%= pie_chart [ 
												["FAILED", @failed[x].count],
												["CLEARED", @cleared[x].count],
												["EXC CLEARED", @excessive[x].count],
												["ULTIMATE CHAIN", @UC[x].count],
												["PERFECT", @PUC[x].count]
											  ],
											colors: [ '#808080', '#00cc00', '#990099', '#ff3399', '#ffff00' ] %>
							</div>
						</div>
					</div>
					</li>
				<% end %>
	  		</ul>
	  	</div>
	</div>
</div>