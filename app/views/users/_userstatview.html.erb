<center><h3>Level <%= level %></h3></center>
<% tiers.each do |tier| %>
	<% if tier.exists? %>
		<h4>
		<% if tier.first.tier == "N" %>
			Not Rated
		<% elsif tier.first.tier == "P" %>
			Personal
		<% else %>
			<%= tier.first.tier %>
		<% end %>	
		</h4>
		<div id = <%= "#{level}_#{tier.first.tier}"%> class="section scrollspy"></div>
		<div class="divider"></div>
		<div class="section">
			<% tier.each do |difficulty| %>
				<span class="parent">
					<img class=<%= difficulty.user_clear(@user.id) %> src= <%= asset_url "song#{difficulty.photo}" %> onclick="update_clear(this, <%= difficulty.id %>)" />
					<img class="difficulty_tag" src = <%= asset_url "difficulty/#{difficulty.difficulty_name}.png" %> />
				</span>
			<% end %>
		</div>
	<% end %>
<% end %>

