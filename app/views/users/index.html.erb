<div class = "container">
  <div class="row">
    <div class = "col s4 offset-s4">
      <table class="highlight">
        <thead>
          <tr>
            <th><h4>All ClearVoltex Users</h4></th>
          </tr>
        </thead>

        <tbody>
          <% @users.each do |user| %>
            <tr>
              <td><%= link_to user.display_name, user %></td>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>

  <br>
</div>
