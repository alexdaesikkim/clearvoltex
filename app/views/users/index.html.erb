<div class = "container">
  <div class="row">
    <div class = "col s8 offset-s2">
      <table class="highlight">
        <thead>
          <tr>
            <th><h4>All Users</h4></th>
          </tr>
        </thead>

        <tbody>
          <% @users.each do |user| %>
            <tr>
              <td><%= link_to user.display_name, user %></td>
              <td><% if user.dan > 0 && user.dan <= 12 %>
                  <img class="user_dan" src= <%= asset_url "dans/#{user.dan}.gif" %> />
                <% else %>
                  <img class="user_dan" src= <%= asset_url "dans/0.gif" %> />
                <% end %>
              </td>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>

  <br>
</div>
